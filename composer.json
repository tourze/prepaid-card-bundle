{
  "name": "tourze/prepaid-card-bundle",
  "description": "Symfony bundle for prepaid card management system",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "easycorp/easyadmin-bundle": "^4",
    "knplabs/knp-menu": "^3.7",
    "monolog/monolog": "^3.1",
    "nesbot/carbon": "^2.72 || ^3",

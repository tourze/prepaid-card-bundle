{
    "name": "tourze/prepaid-card-bundle",
    "description": "预付卡模块",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.1",
        "tourze/enum-extra": "~0.0.5",
        "tourze/easy-admin-attribute": "0.1.*",
        "tourze/doctrine-indexed-bundle": "0.0.*",
        "tourze/doctrine-track-bundle": "0.1.*",
        "tourze/symfony-aop-doctrine-bundle": "0.0.*"
    },
    "autoload": {
        "psr-4": {
            "PrepaidCardBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PrepaidCardBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^2.1"
    }
}
